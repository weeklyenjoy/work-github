<div>商品一覧</div>
<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜き価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
  <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= item.name %></td>
      <td><%= item.tax_out_price %></td>
      <td><%= item.genre.name %></td>
      <td><%= item.is_active == true ? "販売" : "販売中止" %></td>
    </tr>
  <% end %>
  </tbody>
</table>